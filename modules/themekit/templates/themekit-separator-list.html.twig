{#
/**
 * @file
 * Wrap every item into a tag.
 *
 * Available variables:
 * - attributes: HTML attributes for the containing element.
 * - children: The rendered child elements for the link.
 * - url: The url.
 * - options: The options for the link.
 *
 * @see template_preprocess_link()
 *
 * @ingroup themeable
 */
#}
{%- set _first = true -%}
{%- for item in items -%}
  {%- set _item %}{{ item }}{% endset -%}
  {%- if _item|trim != '' -%}
    {%- if _first -%}
      {%- set _first = false -%}
    {%- else -%}
      {{- separator -}}
    {%- endif -%}
    {{- _item -}}
  {%- endif -%}
{%- endfor -%}
