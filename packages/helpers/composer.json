{
    "name": "ock/helpers",
    "description": "Helper functions and classes used by other packages in the ock ecosystem.",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Hennings",
            "email": "andreas@dqxtech.net"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "files": ["functions.php"],
        "psr-4": {
            "Ock\\Helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ock\\Helpers\\Tests\\": "tests/src"
        }
    },
    "require": {
        "php": ">=8.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.38"
    }
}
