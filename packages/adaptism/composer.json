{
    "name": "ock/adaptism",
    "type": "library",
    "description": "Library to map adapter classes.",
    "homepage": "https://github.com/ock/adaptism",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Hennings",
            "email": "andreas@dqxtech.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.2",
        "ock/class-discovery": "self.version",
        "ock/dependency-injection": "self.version",
        "ock/egg": "self.version",
        "ock/helpers": "self.version",
        "psr/container": "^2.0",
        "symfony/dependency-injection": "^6.4"
    },
    "require-dev": {
        "ock/testing": "self.version",
        "phpstan/phpstan": "2.0.x-dev",
        "phpunit/phpunit": "^9.5",
        "symfony/yaml": "^6.2",
        "symfony/config": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Ock\\Adaptism\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ock\\Adaptism\\Tests\\": "tests/src/"
        }
    }
}
