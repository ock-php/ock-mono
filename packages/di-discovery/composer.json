{
    "name": "donquixote/di-discovery",
    "description": "Dependency injection discovery (DID)",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.2",
        "donquixote/class-discovery": "@dev",
        "donquixote/codegen-tools": "@dev",
        "psr/container": "^2.0",
        "psr/log": "^1.1.4|^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/dependency-injection": "^6.4",
        "symfony/yaml": "^6.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Donquixote\\DID\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Donquixote\\DID\\Tests\\": "tests/src"
        }
    },
    "config": {
        "sort-packages": true
    }
}
