{
    "name": "ock/di-discovery",
    "description": "Dependency injection discovery (DID)",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.3",
        "ock/class-discovery": "self.version",
        "ock/codegen-tools": "self.version",
        "ock/helpers": "self.version",
        "ock/reflector-aware-attributes": "self.version",
        "psr/container": "^2.0",
        "psr/log": "^1.1.4|^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.38",
        "symfony/dependency-injection": "^7.1.5",
        "symfony/yaml": "^7.1.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Ock\\DID\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ock\\DID\\Tests\\": "tests/src"
        }
    },
    "config": {
        "sort-packages": true
    }
}
