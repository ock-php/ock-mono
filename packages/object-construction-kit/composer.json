{
    "name": "ock/object-construction-kit",
    "description": "Object Construction Kit (Ock)",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": ">=8.2",
        "ock/adaptism": "*",
        "ock/class-discovery": "@dev",
        "ock/di-discovery": "@dev",
        "ock/helpers": "@dev",
        "psr/log": "^1.1.4|^2.0|^3.0",
        "symfony/config": "^6.4"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-libxml": "*",
        "ock/testing": "self.version",
        "phpunit/phpunit": "^9.5",
        "symfony/dependency-injection": "^6.4",
        "symfony/expression-language": "^6.4",
        "symfony/yaml": "^6.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ock\\Ock\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ock\\Ock\\Tests\\": "tests/src"
        }
    }
}
