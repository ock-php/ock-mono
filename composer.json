{
    "name": "donquixote/object-construction-kit",
    "description": "Object Construction Kit (Ock)",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "path",
            "url": "../../phplib/adaptism"
        },
        {
            "type": "path",
            "url": "../../phplib/adaptism-d8"
        },
        {
            "type": "path",
            "url": "../../phplib/annotation-parser"
        },
        {
            "type": "path",
            "url": "../../phplib/callback-reflection"
        },
        {
            "type": "path",
            "url": "../../phplib/class-discovery"
        },
        {
            "type": "path",
            "url": "../../phplib/containerkit"
        },
        {
            "type": "path",
            "url": "../../phplib/factory-discovery"
        },
        {
            "type": "path",
            "url": "../../phplib/factory-reflection"
        },
        {
            "type": "path",
            "url": "../../phplib/nicetrace"
        },
        {
            "type": "path",
            "url": "../../phplib/reflectionkit"
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer/installers": "^1.9",
        "donquixote/annotation-parser": "@dev",
        "donquixote/callback-reflection": "@dev",
        "donquixote/class-discovery": "@dev",
        "donquixote/containerkit": "*",
        "donquixote/factory-reflection": "dev-master",
        "donquixote/nicetrace": "*",
        "donquixote/reflectionkit": "*",
        "ondrejmirtes/better-reflection": "dev-master",
        "psr/log": "^1.1",
        "symfony/polyfill-php80": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "symfony/yaml": "^5.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Donquixote\\Ock\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Donquixote\\Ock\\Tests\\": "tests/src"
        }
    }
}
