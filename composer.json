{
    "name": "donquixote/di-discovery",
    "description": "Dependency injection discovery (DID)",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "path",
            "url": "../../phplib/class-discovery"
        }
    ],
    "require": {
        "php": ">=8.1",
        "donquixote/class-discovery": "@dev",
        "psr/log": "^1.1.4|^2.0|^3.0"
    },
    "require-dev": {
        "composer/installers": "^2.0",
        "phpunit/phpunit": "^9.5",
        "symfony/yaml": "^6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Donquixote\\DID\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Donquixote\\DID\\Tests\\": "tests/src"
        }
    }
}
